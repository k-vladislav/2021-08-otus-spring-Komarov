<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="quizDao" class="ru.otus.spring.dao.QuizDaoImpl">
        <constructor-arg name="sourcePath" value="/Quizzes/Questions_Answers.csv"/>

    </bean>
    <bean id="quizService" class="ru.otus.spring.service.QuizServiceImpl">
        <constructor-arg name="dao" ref="quizDao"/>
    </bean>
</beans>